        Image:
            source: "images/r_contact.png"
            size_hint: 0.7, 0.7
            pos_hint: {"center_x": .45, "center_y": .85}
        MDLabel:
            markup: True
            text: "Feedback"
            font_name: "Poppins-Bold"
            font_size: 22
